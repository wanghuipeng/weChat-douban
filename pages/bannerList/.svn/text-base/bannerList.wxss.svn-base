.page{display: flex;flex-direction: column;height:100%;}
.top .search-bar{background-color: #e6e6e6;display:flex;flex-direction: row;padding:10px 0px;height: 30px;line-height: 30px;}
.top .search-bar .search-l{display: flex;flex-direction: row;align-items: center;background-color: #fff;height: 30px;width: 85%;margin-left: 15px;border-radius: 4px;padding: 0 5px}
.top .search-bar .search-l icon{vertical-align: middle;line-height: 40px;margin-top: 13px}
.top .search-bar .search-l input{padding: 0 10px 0 5px;width: 100%}
.top .search-bar .search-r{width: 15%;margin-right: 15px;text-align: center}
.top .search-bar .search-r text{color: orangered;font-size: 14px;font-weight:600}
.top .search-list .item{height: 40px;line-height: 40px;padding: 0 35px;border-bottom: 1rpx solid #eee;}
.top .search-list .item:first-child{height: 30px;line-height: 30px;font-size: 12px}
.top .search-list .item .title{color: #999}
.filter{display: flex;flex-direction: row;padding: 10px 0;border-bottom: 1rpx solid #eee;}
.filter .item{flex: 1;text-align: center;border-left: 1px solid #eee}
.filter .item:first-child{border-left: none}
.filter .item text{margin-right: 5px}
.filter .item image{width: 10px;height: 8px}
.filter .item .rotate{transform: rotate3d(0,0,1,-180deg);transition: transform 300ms ease-in-out;}
.s_flowindow{
  display: none;
  position: fixed;
  top: 92px;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0,0.3);
  z-index: 10
}
 .s_content{
   display: none;
  position: absolute;
  z-index: 100;
  width: 100%;
  background-color: #fff;
  max-height:259px;
  overflow:auto;
}
.s_more{
  display: none;
  position: relative;
  z-index: 100;
  overflow: hidden;
}
.show{
  display: block !important;
}
.s_content .s_list{padding: 8px 15px;border-top: 1px solid #eee;}
.s_content .s_list:first-child{border-top: none}